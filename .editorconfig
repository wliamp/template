root = true
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 4
[*.java]
continuation_indent_size = 8
[*.kt]
continuation_indent_size = 8
max_line_length = 120
[*.yml]
indent_size = 2
[*.yaml]
indent_size = 2
[*.md]
trim_trailing_whitespace = false
